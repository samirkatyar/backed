{
  "name": "express-service-bootstrap",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www"
  },
  "engines": {
    "node": ">14"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
