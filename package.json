    "morgan": "1.10.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/mongoose": "5.10",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
